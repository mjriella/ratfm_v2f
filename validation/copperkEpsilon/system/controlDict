FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     ratfmFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10;

deltaT          0.05e-4;

writeControl    adjustableRunTime;

writeInterval   1e-1;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable on;

adjustTimeStep  yes;

maxCo           1;

maxDeltaT       1;


functions
{
    fieldAverage1
    {
        type            fieldAverage;
        functionObjectLibs ( "libfieldFunctionObjects.so" );
        outputControl   outputTime;
        fields
        (
            U1
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
            }
            U2
            {
                 mean        on;
                 prime2Mean  on;
                 base        time;
            }
            k1
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
            }
            k2
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
            }
            Theta
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
            }
        );
    }
}
// ************************************************************************* //
